#Programa nao tem erro sintático

:DEC
parametro:INT
fatorial:INT
ola:REA
:PROG 
# Calcula o fatorial de um número inteiro
LER parametro
LER ola
fatorial := 1000000000000000000.100000000000000001
ola := 2147483647.0
SE parametro == 0 ENTAO fatorial := 1 +oi
ENQTO parametro > 1
 INI
  fatorial := fatorial * (parametro - 1) 
  parametro := parametro - 1
 FIM
IMPRIMIR fatorial
IMPRIMIR ola
IMPRIMIR "Oi"
